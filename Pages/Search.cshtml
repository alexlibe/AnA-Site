@page
@model SearchModel
@{
    ViewData["Title"] = "Search";
}

<p>Search site</p>
<form method="get">
    <span asp-validation-for="Search" style="color: red; display: block;"></span>
    <input type="search" placeholder="Search here..." asp-for="Search">
    <button type="submit" text="Submit" asp-page="Search"></button>
</form>

@if (Model.Videos != null)
{
    <p>@Model.Videos.Length</p>
}